    <style>

        body, h1, h2, h3, h4, p, div, span {
            font-family: 'Vollkorn', serif !important;
        }

        h2 {
            font-size: 1.6em;
        }

        h3 {
            font-size: 1.3em;
        }

        .axis {
            font: 10px sans-serif;
        }

        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }

        rect.bordered {
            stroke: #E6E6E6;
            stroke-width: 2px;
        }

        text.mono {
            font-size: 9pt;
            font-family: Consolas, courier;
            fill: #aaa;
        }

        text.axis-workweek {
            fill: #000;
        }

        text.axis-worktime {
            fill: #000;
        }

        i {
            color: #5CB85C;
        }

    </style>

    <script src="http://d3js.org/d3.v3.js"></script>
</head>
<body>
<div class="blog-masthead">
    <div class="container">
        <nav class="blog-nav">
            <a class="blog-nav-item {% block home_active %} {% endblock %}" href="{{ path('home_page') }}">Home</a>
            <a class="blog-nav-item {% block history_active %} {% endblock %}" href="{{ path('history') }}">Load
                History</a>
            <a class="blog-nav-item {% block api_active %} {% endblock %}" href="{{ path('api') }}">API</a>
            <a class="blog-nav-item {% block about_active %} {% endblock %}" href="{{ path('about') }}">About</a>
            <a class="blog-nav-item {% block faq_active %} {% endblock %}" href="{{ path('faq') }}">FAQ</a>
            <a class="blog-nav-item {% block contact_active %} {% endblock %}" href="{{ path('contact') }}">Contact</a>
            <a class="blog-nav-item" href="https://github.com/mos3abof/gridstatusnow" target="_blank">Github</a>
        </nav>
    </div>
</div>


<div class="container">

    {% block content %}{% endblock %}


</div>
<!-- /.container -->

